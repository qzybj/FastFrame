<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.frame.fastframe">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:name="com.frame.fastframe.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.frame.fastframe.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.frame.fastframe.module.home.ui.HomeActivity"/>
        <activity android:name="com.frame.fastframe.module.communicate.ui.CommunicateActivity"/>
        <activity android:name="com.frame.fastframe.module.news.ui.NewsActivity"/>
        <activity android:name="com.frame.fastframe.ui.simple.ui.SimpleBaseAdapterActivity"/>
        <activity android:name="com.frame.fastframe.ui.simple.ui.SimpleMultiBaseAdapterActivity"/>
        <!--moduls-->
        <activity android:name="com.frame.fastframe.module.product.ui.ProductActivity"/>
    </application>

</manifest>
