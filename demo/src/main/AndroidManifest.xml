<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="earlll.com.testdemoall">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/style_activty_custom">
        <activity
            android:name="earlll.com.testdemoall.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboard|keyboardHidden"
            android:theme="@style/style_activty_custom">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="earlll.com.testdemoall.ui.SimpleBaseAdapterActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboard|keyboardHidden"
            android:theme="@style/style_activty_custom" />

        <activity
            android:name="earlll.com.testdemoall.ui.TestApplicationActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboard|keyboardHidden"
            android:theme="@style/style_activty_custom" />
        <activity
            android:name="earlll.com.testdemoall.ui.SimpleSwipeRefreshLayoutActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboard|keyboardHidden"
            android:theme="@style/style_activty_custom" />
        <activity
            android:name="earlll.com.testdemoall.ui.SimpleWebViewPullrefreshActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboard|keyboardHidden"
            android:theme="@style/style_activty_custom" />
        <activity
            android:name="earlll.com.testdemoall.ui.SimplePullRefreshLayoutActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboard|keyboardHidden"
            android:theme="@style/style_activty_custom" />
        <activity
            android:name="earlll.com.testdemoall.ui.SimpleHorizontalListViewActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboard|keyboardHidden"
            android:theme="@style/style_activty_custom" />
        <activity
            android:name="earlll.com.testdemoall.ui.SimpleFragmentActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboard|keyboardHidden"
            android:theme="@style/style_activty_custom" />
        <activity
            android:name=".ui.EventBusReciveActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboard|keyboardHidden"
            android:theme="@style/style_activty_custom" />
        <activity
            android:name=".ui.EventBusSendActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboard|keyboardHidden"
            android:theme="@style/style_activty_custom" />
        <activity
            android:name=".ui.SimpleSensorActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            android:configChanges="keyboard|keyboardHidden"
            android:theme="@style/style_activty_custom" />
    </application>

</manifest>